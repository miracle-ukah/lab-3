#include <iostream>
#include "Header.h"
#include <math.h>

using namespace std;

double tst(int x, int n)
{
    return pow(x, 2) + x + n;
}
double yFunc(int x, int n)
{
    double y = 0;
    if (x <= 0)
    {
        for (double i = 2; i <= (n - 1); i++)
        {
            y += (x - i) / i;
        }
    }
    else
    {
        for (int i = 0; i <= (n - 1); i++)
        {
            double s = 0;
            for (int j = 0; j <= n; j++)
            {
                s += (i + 1) / (j + x);
            }
            y *= s;
        }
    }
    return y;
}
int main()
{
    double Begin, End, Interval, x;
    cout << "Enter the beginning " << endl;
    std::cin >> Begin;
    cout << "Enter the end " << endl;
    std::cin >> End;
    cout << "Enter the interval " << endl;
    std::cin >> Interval;
    cout << "Enter the x " << endl;
    std::cin >> x;

    print(Begin, End, Interval, x, yFunc);
    print(Begin, End, Interval, x, tst);
}
