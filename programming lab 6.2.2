#include "Header.h"
#include <iostream>
#include <cmath>

double greatestRoot(func f, int Begin, int Stop, int Step, int n)
{
    double max = f(Begin, n);
    for (double i = Begin; i <= Stop; i += Step)
    {
        double bigger = f(i, n);
        if (bigger > max)
        {
            max = bigger;
        }
    }
    return max;
}
